@model List<Domain>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Role Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var domain in Model)
        {
            <tr>
                <td>@(domain.Role.Name)</td>
                <td>
                    <a href="#" class="btn btn-warning">Update</a>
                    <a href="#" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

